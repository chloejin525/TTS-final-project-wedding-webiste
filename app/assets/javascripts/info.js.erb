function initMap() {
  var venueCoords = new google.maps.LatLng(35.835991, -78.759460);
  
  var mapOptions = {
    zoom: 14,
    center: venueCoords,
    scrollwheel: false
  };
    
  var map = new google.maps.Map(document.getElementById('venue-map-canvas'), mapOptions);  

  var contentString = 'Reedy Creek Entrance';

  var infowindow = new google.maps.InfoWindow({
    content: contentString
  });

  
  var icon = {
      url: "/assets/wedding_icon.png",
      scaledSize: new google.maps.Size(50, 50), // scaled size
      origin: new google.maps.Point(0,0), // origin
      anchor: new google.maps.Point(0, 0) // anchor
  };

  var userMarker = new google.maps.Marker({
    position: venueCoords,
    map: map,
    icon: icon
  }); 


  userMarker.addListener('click', function(){
    infowindow.open(map, userMarker);
  })
}



        